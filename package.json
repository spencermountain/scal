{
  "name": "scal",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "description": "calendar output",
  "version": "0.0.1",
  "main": "./src/index.js",
  "unpkg": "./builds/scal.js",
  "module": "./builds/scal.mjs",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./builds/scal.js",
      "import": "./builds/scal.mjs",
      "default": "./builds/scal.js"
    }
  },
  "bin": {
    "scal": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/scal.git"
  },
  "homepage": "https://github.com/spencermountain/scal",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rollup -c --silent",
    "test": "tape \"./tests/**/*.test.js\" | tap-dancer",
    "testb": "npm run test:smoke && TESTENV=prod npm run test",
    "watch": "amble ./scratch.js"
  },
  "files": [
    "builds/"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "spacetime": "^6.16.3"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "amble": "1.3.0",
    "rollup": "2.61.1",
    "rollup-plugin-terser": "7.0.2",
    "tap-dancer": "0.3.4",
    "tape": "5.3.2"
  },
  "eslintIgnore": [
    "builds/*.js"
  ],
  "license": "MIT"
}